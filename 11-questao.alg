algoritmo "Questao11"

var
num1, num2, num3: real 
media: real 

inicio

    escreval("entre com o primeiro número: ")
    leia(num1)
    escreval("entre com o segundo número :")
    leia(num2)
    escreval("entre com o terceiro número : ")
    leia(num3)

        media<- (num1+num2+num3)/3

    escreval("A média aritimética dos 3 números inseridos é : ", media)



fimalgoritmo